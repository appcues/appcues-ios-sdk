{"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/com.appcues.sdk\/documentation\/AppcuesKit\/Appcues\/Config\/enableUniversalLinks(_:)"},"sections":[],"metadata":{"roleHeading":"Instance Method","fragments":[{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"text":"enableUniversalLinks","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"s:Sb","text":"Bool","kind":"typeIdentifier"},{"text":") -> ","kind":"text"},{"text":"Self","kind":"typeIdentifier"}],"role":"symbol","modules":[{"name":"AppcuesKit"}],"externalID":"s:10AppcuesKit0A0C6ConfigC20enableUniversalLinksyAEXDSbF","symbolKind":"method","title":"enableUniversalLinks(_:)"},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"attribute","text":"@discardableResult"},{"text":" ","kind":"text"},{"kind":"attribute","text":"@objc"},{"kind":"text","text":" "},{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"enableUniversalLinks"},{"text":"(","kind":"text"},{"text":"_","kind":"externalParam"},{"text":" ","kind":"text"},{"text":"enabled","kind":"internalParam"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","text":"Bool","preciseIdentifier":"s:Sb"},{"text":") -> ","kind":"text"},{"text":"Self","kind":"typeIdentifier"}],"platforms":["iOS"],"languages":["swift"]}]},{"parameters":[{"name":"enabled","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Whether universal link handling is enabled."}]}]}],"kind":"parameters"},{"kind":"content","content":[{"level":2,"text":"Return Value","type":"heading","anchor":"return-value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The "},{"type":"codeVoice","code":"Configuration"},{"text":" object.","type":"text"}]}]},{"content":[{"type":"heading","level":2,"anchor":"discussion","text":"Discussion"},{"inlineContent":[{"type":"text","text":"When this property is enabled, the SDK will pass web links that are triggered by an experience back to the host"},{"text":" ","type":"text"},{"text":"application to process. The host application should implement the ","type":"text"},{"code":"application(_:continue:restorationHandler:)","type":"codeVoice"},{"text":" ","type":"text"},{"text":"in its ","type":"text"},{"code":"ApplicationDelegate","type":"codeVoice"},{"type":"text","text":" to return "},{"code":"true","type":"codeVoice"},{"text":" if the link provided in the given ","type":"text"},{"type":"codeVoice","code":"NSUserActivity"},{"text":" has been handled","type":"text"},{"type":"text","text":" "},{"text":"as a universal link inside the current application, or ","type":"text"},{"code":"false","type":"codeVoice"},{"type":"text","text":" if not. When a value of "},{"type":"codeVoice","code":"true"},{"type":"text","text":" is returned, this informs the SDK"},{"type":"text","text":" "},{"text":"that no further link handling is needed. When a value of ","type":"text"},{"type":"codeVoice","code":"false"},{"text":" is returned, the SDK will continue handling the link and open","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the web destination in the browser."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"When this property is disabled, any web links triggered by an experience will always be opened in the browser, regardless of","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"whether it was a universal link that may have been intended to deep link to another screen in the app."}]},{"inlineContent":[{"text":"The default value for this configuration is ","type":"text"},{"code":"true","type":"codeVoice"},{"type":"text","text":"."}],"type":"paragraph"}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/appcueskit\/appcues\/config\/enableuniversallinks(_:)"]}],"schemaVersion":{"patch":0,"major":0,"minor":3},"hierarchy":{"paths":[["doc:\/\/com.appcues.sdk\/documentation\/AppcuesKit","doc:\/\/com.appcues.sdk\/docume