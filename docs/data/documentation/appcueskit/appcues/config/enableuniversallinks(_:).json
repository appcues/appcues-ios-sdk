{"sections":[],"identifier":{"url":"doc:\/\/com.appcues.sdk\/documentation\/AppcuesKit\/Appcues\/Config\/enableUniversalLinks(_:)","interfaceLanguage":"swift"},"abstract":[{"type":"text","text":"Set the universal link handling preference for the configuration."}],"metadata":{"roleHeading":"Instance Method","role":"symbol","externalID":"s:10AppcuesKit0A0C6ConfigC20enableUniversalLinksyAEXDSbF","title":"enableUniversalLinks(_:)","modules":[{"name":"AppcuesKit"}],"fragments":[{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"text":"enableUniversalLinks","kind":"identifier"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"s:Sb","text":"Bool"},{"kind":"text","text":") -> "},{"kind":"typeIdentifier","text":"Self"}],"symbolKind":"method"},"hierarchy":{"paths":[["doc:\/\/com.appcues.sdk\/documentation\/AppcuesKit","doc:\/\/com.appcues.sdk\/documentation\/AppcuesKit\/Appcues","doc:\/\/com.appcues.sdk\/documentation\/AppcuesKit\/Appcues\/Config"]]},"schemaVersion":{"major":0,"patch":0,"minor":3},"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"languages":["swift"],"tokens":[{"text":"@discardableResult","kind":"attribute"},{"text":" ","kind":"text"},{"kind":"attribute","text":"@objc"},{"kind":"text","text":" "},{"text":"func","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"enableUniversalLinks"},{"kind":"text","text":"("},{"text":"_","kind":"externalParam"},{"text":" ","kind":"text"},{"kind":"internalParam","text":"enabled"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:Sb","kind":"typeIdentifier","text":"Bool"},{"text":") -> ","kind":"text"},{"text":"Self","kind":"typeIdentifier"}]}]},{"kind":"parameters","parameters":[{"name":"enabled","content":[{"type":"paragraph","inlineContent":[{"text":"Whether universal link handling is enabled.","type":"text"}]}]}]},{"kind":"content","content":[{"anchor":"return-value","text":"Return Value","level":2,"type":"heading"},{"type":"pa