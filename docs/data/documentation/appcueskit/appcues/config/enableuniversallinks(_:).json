{"identifier":{"url":"doc:\/\/com.appcues.sdk\/documentation\/AppcuesKit\/Appcues\/Config\/enableUniversalLinks(_:)","interfaceLanguage":"swift"},"metadata":{"roleHeading":"Instance Method","externalID":"s:10AppcuesKit0A0C6ConfigC20enableUniversalLinksyAEXDSbF","symbolKind":"method","fragments":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"enableUniversalLinks","kind":"identifier"},{"text":"(","kind":"text"},{"text":"Bool","kind":"typeIdentifier","preciseIdentifier":"s:Sb"},{"text":") -> ","kind":"text"},{"text":"Self","kind":"typeIdentifier"}],"modules":[{"name":"AppcuesKit"}],"role":"symbol","title":"enableUniversalLinks(_:)"},"schemaVersion":{"major":0,"patch":0,"minor":3},"sections":[],"kind":"symbol","primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"attribute","text":"@discardableResult"},{"kind":"text","text":" "},{"kind":"attribute","text":"@objc"},{"kind":"text","text":" "},{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"enableUniversalLinks"},{"kind":"text","text":"("},{"kind":"externalParam","text":"_"},{"kind":"text","text":" "},{"kind":"internalParam","text":"enabled"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"s:Sb","text":"Bool"},{"kind":"text","text":") -> "},{"kind":"typeIdentifier","text":"Self"}],"platforms":["iOS"],"languages":["swift"]}]},{"parameters":[{"content":[{"type":"paragraph","inlineContent":[{"text":"Whether universal link handling is enabled.","type":"text"}]}],"name":"enabled"}],"kind":"parameters"},{"kind":"content","content":[{"level":2,"text":"Return Value","anchor":"return-value","type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The "},{"type":"codeVoice","code":"Configuration"},{"type":"text","text":" object."}]}]},{"kind":"content","content":[{"type":"heading","text":"Discussion","level":2,"anchor":"discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"When this property is enabled, the SDK will pass web links that are triggered by an experience back to the host"},{"type":"text","text":" "},{"type":"text","text":"application to process. The host application should implement the "},{"code":"application(_:continue:restorationHandler:)","type":"codeVoice"},{"type":"text","text":" "},{"type":"text","text":"in its "},{"code":"ApplicationDelegate","type":"codeVoice"},{"type":"text","text":" to return "},{"code":"true","type":"codeVoice"},{"type":"text","text":" if the link provided in the given "},{"code":"NSUserActivity","type":"codeVoice"},{"type":"text","text":" has been handled"},{"type":"text","text":" "},{"type":"text","text":"as a universal link inside the current application, or "},{"code":"false","type":"codeVoice"},{"type":"text","text":" if not. When a value of "},{"code":"true","type":"codeVoice"},{"type":"text","text":" is returned, this informs the SDK"},{"type":"text","text":" "},{"type":"text","text":"that no further link handling is needed. When a value of "},{"code":"false","type":"codeVoice"},{"type":"text","text":" is returned, the SDK will continue handling the link and open"},{"type":"text","text":" "},{"type":"text","text":"the web destination in the browser."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"When this property is disabled, any web links triggered by an experience will always be opened in the browser, regardless of"},{"type":"text","text":" "},{"type":"text","text":"whether it was a universal link that may have been intended to deep link to another screen in the app."}]},{"type":"paragraph","inlineContent":[{"text":"The default value for this configuration is ","type":"text"},{"code":"true","type":"codeVoice"},{"text":".","type":"text"}]}]}],"hierarchy":{"paths":[["doc:\/\/com.appcues.sdk\/documentation\/AppcuesKit","doc:\/\/com.appcues.sdk\/documentation\/AppcuesKit\/Appcues","doc:\/\/com.appcues.sdk\/documentation\/AppcuesKit\/Appcues\/Config"]]},"variants":[{"traits